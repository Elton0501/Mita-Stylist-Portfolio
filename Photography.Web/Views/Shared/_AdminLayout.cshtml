<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title></title>

    <link href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap/5.2.3/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/all.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    <link href="~/Content/jquery.dataTables.min.css" rel="stylesheet" />
    <!-- Custom styles for this template-->
    <link href="~/Content/sb-admin-2.min.css" rel="stylesheet" />
    <link href="~/Content/sweetalert2.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-icons/1.8.3/font/bootstrap-icons.min.css" />

    <script src="~/Scripts/jquery-3.4.1.min.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap/5.2.3/js/bootstrap.bundle.min.js"></script>
    <script src="~/Scripts/Chart/Chart.js"></script>
    <script src="~/Scripts/jquery.dataTables.min.js"></script>

    <style>
        .error {
            color:red;
        }
        .loginbtn {
            font-size: 13px;
            margin: 0;
            padding: 4px 10px;
        }

        .table .thead-dark th {
            text-transform: uppercase !important;
        }

        .dataTables_wrapper .dataTables_paginate .paginate_button {
            box-sizing: border-box;
            display: inline-block;
            padding: 0.25em 0.61em;
            margin-left: 2px;
            text-align: center;
            text-decoration: none !important;
            cursor: pointer;
            color: #333 !important;
            border: none;
            border-radius: 2px;
            margin-top: 5px;
            font-size: 15px;
        }

            .dataTables_wrapper .dataTables_paginate .paginate_button.current, .dataTables_wrapper .dataTables_paginate .paginate_button.current:hover {
                color: #fbfbfb !important;
                border: none;
                background: var(--blue);
                outline: none !important;
            }

            .dataTables_wrapper .dataTables_paginate .paginate_button:hover {
                color: var(--blue) !important;
                border: none;
                background: #e3ebf7;
            }

        .dataTables_info {
            margin-top: 5px;
            font-size: 14px;
        }

        .buttons-copy, .buttons-excel, .buttons-csv, .buttons-print {
            color: #fff;
            background-color: #4e73df;
            border-color: #4e73df;
            font-size: 13px;
            margin: 0;
            padding: 6px 15px 4px 10px;
            border: none;
            outline: none !important;
        }

        .buttons-excel {
            background: var(--cyan) !important;
        }

        .buttons-csv {
            background: var(--danger) !important;
        }

        .buttons-print {
            background: var(--dark) !important;
        }

        .cont {
            margin: 5px;
        }

        .adminpage-head {
            padding: 0rem 2rem;
        }

        thead, tr {
            text-align: center;
        }

        table {
            font-size: 11px;
        }

        .col-md-10 {
            max-width: 100% !important;
        }

        .sidebar-brand-icon img {
            width: 79% !important;
        }

        @@media(max-width:700px) {
            .adminpage-head {
                padding: 0rem 0.5rem;
            }
        }
    </style>

</head>

<body id="page-top">



    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Sidebar -->
        @Html.Partial("_AdminSidebarPartial")
        <!-- End of Sidebar -->
        <!-- Content Wrapper -->

        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Topbar -->
                @Html.Partial("_AdminNavPartial")
                <!-- End of Topbar -->
                <!-- Begin Page Content -->
                <div class="cont">
                    @RenderBody()
                </div>
                <!-- /.container-fluid -->
            </div>


            <!-- End of Main Content -->
            <!-- Footer -->
            <footer class="sticky-footer bg-white">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto">
                        <span>Copyright &copy; Your Website 202</span>
                    </div>
                </div>
            </footer>
            <!-- End of Footer -->

        </div>

        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->
    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="bi bi-arrow-up"></i>
    </a>

    <script src="~/Scripts/jquery.easing.min.js"></script>
    <script src="~/Scripts/sb-admin-2.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.6.4/js/dataTables.buttons.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.6.4/js/buttons.html5.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.6.4/js/buttons.print.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
    <script src="~/Scripts/sweetalert2.all.min.js"></script>
    <script src="~/Content/ckeditor/ckeditor.js"></script>
</body>

</html>